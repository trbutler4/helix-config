{
  "name": "tree-sitter-pem",
  "version": "0.1.0",
  "description": "PEM grammar for tree-sitter",
  "author": "Matthew Toohey <contact@mtoohey.com>",
  "dependencies": {
    "nan": "^2.17.0"
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.20.7"
  },
  "main": "bindings/node",
  "tree-sitter": [
    {
      "scope": "source.pem",
      "injection-regex": "pem",
      "file-types": [
        "pem",
        "cert",
        "crt"
      ]
    }
  ]
}
